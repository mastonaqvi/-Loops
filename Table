#include <iostream> //library file
using namespace std;
int main() {
    int num,tab; // declare veriable

    cout << "Enter Numbers: ";
    cin >> num; //get input from the user
    
    for (int i = 1; i <= 10; i++)  //loop  from 1 to 10
    {
        tab = num * i; // calculate the table
        cout << num << "*"<<i<<"= " << tab<<endl; // print  table
    }
    return 0; //successfully run
}
